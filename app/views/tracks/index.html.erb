<div class="row">
  <div class="small-12 medium-6 medium-centered columns">
    <div class="panel tracks-list">
      <h3>Tracks</h3>
      <ul class="no-bullet">
        <% @tracks.each do |track| %>
          <li>
            <%= link_to_igv(track) %>
            <%= link_to track.name, track, class: "track" %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
