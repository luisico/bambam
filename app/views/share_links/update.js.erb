<% if @share_link.errors.any? %>
  $('#edit_share_link_<%= @share_link.id %>').remove();
  $('#share_link_<%= @share_link.id %>').append('<%= j render("form") %>');
  $('#share_link_expires_at').datepicker({ dateFormat: "'Expires on' d, MM yy" })
<% else %>
  $('#share-links').html("<%= escape_javascript(render(@share_link.track.share_links)) %>");
  $('.expired').parent().hide()
<% end %>
